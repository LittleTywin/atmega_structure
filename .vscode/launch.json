{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/Target/Debug/test.elf",
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/avr-gdb",
			"miDebuggerServerAddress": "localhost:1442",
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": true,
			
			//"preLaunchTask": "Debug server",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",    
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
                ],
		}
	]
}